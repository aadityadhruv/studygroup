    "import pandas as pd\n",
    "import os\n",
    "\n",
    "for file in os.listdir(\"CSV\"):\n",
    "    pull = 'CSV/'+file\n",
    "    push = 'AAAAA/'+pull[4:][:-4]+'.json'\n",
    "\n",
    "    df = pd.read_csv (pull)\n",
    "\n",
    "    df.to_json (push)\n"
